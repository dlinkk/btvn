#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
      int c  ; 
      cin >> c ; 
      vector<int> v(c);
      for (unsigned int i = 0 ; i < v.size() ; i++){
          cin >> v[i] ; 
      }
      int q  , m  , n ;
      cin >> q >> m >> n ; 
      v.erase(v.begin()+q-1  , v.begin()+q );
     v.erase(v.begin()+m-1 , v.begin()+n-1);
      cout << v.size()<<endl;
      for (unsigned int i = 0 ; i < v.size() ; i++){
          cout << v[i] << " ";
      }
      
    return 0;
}
